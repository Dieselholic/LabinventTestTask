{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoggerServiceOptions": {
    "LogsFilePath": "Logs//FileParserLogs.txt",
    "IsStackTraceOn": false
  },
  "AllowedHosts": "*",
